{%  if event %}
    {% if event.validated==1 %}
        <div style="float: left;width: 50%">

            <article class="card" style="background: #65c178;text-align: center">
                    <img class="thumb"
                         src="{{ asset('bundles/uploads/brochures/'~event.brochure ) }}">
                <div class="infos">
                    <h2 class="title">{{ event.name }}</h2>
                    <h3 class="date">{{event.beginningDate| date('F') }} {{ event.beginningDate| date('d') }},{{ event.beginningDate | date('Y') }}</h3>
                    <h3 class="seats">{{ event.address }}</h3>
                    <p class="txt">
                        {{ event.description }}
                    </p>
                    <p>            <a href="{{ path('event_show', { 'id':event.id }) }}">Read More</a>
                    </p>
                    <h3 class="details">event details</h3>
                </div>
            </article>
        </div>
        {% endif %}
        {% endif %}
